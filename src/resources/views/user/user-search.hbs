		<form class="form" action="" method="post">
      <div class="form-collect">
        <div class="form-box">
          <label for="">Từ sân bay</label>
          <select name="" id="">
              {{#each bookings}}
            <option value="{{this.trip_details.pickup_location}}">{{this.trip_details.pickup_location}}</option>
                {{/each}}
          </select>
        </div>
        <div class="form-box">
          <label for="">Đến khu vực, địa chỉ nhà</label>
          <select name="" id="">
             {{#each bookings}}
            <option value="{{this.trip_details.dropoff_location}}">{{this.trip_details.dropoff_location}}</option>
                {{/each}} 
          </select>
        </div>
        <div class="form-box">
          <label for="">Ngày đón</label>
          <input type="date" />
        </div>
        <div class="form-box">
          <label for="">Giờ đón</label>
          <input type="time" />
        </div>
      </div>
      <button type="submit">Tìm kiếm</button>
      <div class="clear-both"></div>
		</form>

